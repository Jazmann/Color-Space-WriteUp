   FractionBox[
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"x", "-", "x0"}], ")"}], "2"], 
    SuperscriptBox["a", "2"]], "+", 
   FractionBox[
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"y", "-", "y0"}], ")"}], "2"], 
